{
  "name": "feynquest-ext",
  "displayName": "FeynQuest",
  "description": "Your Ai coding Assistant",
  "version": "0.1.1",
  "publisher": "SF-24",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./extension.js",
  "scripts": {
    "lint": "eslint ."
  },
  "activationEvents": [
    "onCommand:feynquest.ask"
  ],
  "contributes": {
    "commands": [
      {
        "command": "feynquest.ask",
        "title": "Ask FeynQuest"
      }
    ],
    "keybindings": [
      {
        "command": "feynquest.ask",
        "key": "ctrl+m",
        "mac": "cmd+m",
        "when": "editorTextFocus"
      }
    ]
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "eslint": "^8.50.0"
  },
  "dependencies": {
    "openai": "^4.73.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastien509/FeynQuest-Extension"
  }
}
