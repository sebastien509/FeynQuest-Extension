{
  "name": "feynquest-ext",
  "displayName": "FeynQuest",
  "description": "Your AI coding assistant",
  "version": "0.1.0",
  "publisher": "S.F.",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./extension.js",
  "activationEvents": ["*"],
  "contributes": {
    "commands": [
      {
        "command": "feynquest.ask",
        "title": "Ask FeynQuest"
      }
    ],
    "keybindings": [
      {
        "command": "feynquest.ask",
        "key": "ctrl+m",
        "mac": "cmd+m",
        "when": "editorTextFocus"
      }
    ]
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "eslint": "^8.50.0"
  },
  "dependencies": {
    "openai": "^4.73.1"
  }
}
